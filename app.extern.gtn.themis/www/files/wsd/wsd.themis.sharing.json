{
	"shareDocument": 
	{
		"url": "/backmeup-service-rest/sharing/add/json?withUserId={withUserId}&policyType=Document&policyValue={policyValue}",
		"method": "post",
		"timeout": "30000"
	},

	"shareBackup": 
	{
		"url": "/backmeup-service-rest/sharing/add/json?withUserId={withUserId}&policyType=Backup&policyValue={policyValue}",
		"method": "post",
		"timeout": "30000"
	},

	"shareNew": 
	{
		"url": "/backmeup-service-rest/sharing/add/json?withUserId={withUserId}&policyType=AllFromNow&policyValue={policyValue}",
		"method": "post",
		"timeout": "30000"
	},

	"shareAll": 
	{
		"url": "/backmeup-service-rest/sharing/add/json?withUserId={withUserId}&policyType=AllInklOld&policyValue={policyValue}",
		"method": "post",
		"timeout": "30000"
	},

	"ownedShares": 
	{
		"url": "/backmeup-service-rest/sharing/owned",
		"method": "get",
		"timeout": "30000"
	},

	"incomingShares": 
	{
		"url": "/backmeup-service-rest/sharing/incoming",
