{
	"name":"RestClient",
	"shortname":"rc",
	"webservices":{
		"template":{
			"url":"string url",
			"method":"string method",
			"timeout":"int timeout"
		},
		"login":{
			"url":"/users/{username}/login?password={password}",
			"method":"post",
			"timeout":15000
		},
		"register":{
			"url":"/users/{username}/register?password={password}&email={email}&keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"newVerficiationEmail":{
			"url":"/users/{username}/newVerificationEmail",
			"method":"get",
			"timeout":15000
		},
		"verifyEmail":{
			"url":"/users/{verificationKey}/verifyEmail",
			"method":"get",
			"timeout":15000
		},
		"userDetails":{
			"url":"/users/{username}",
			"method":"get",
			"timeout":15000
		},
		"deleteUser":{
			"url":"/users/{username}",
			"method":"delete",
			"timeout":15000
		},
		"changeUser":{
			"url":"/users/{oldUsername}?username={username}&oldPassword={oldPassword}&oldKeyRing={oldKeyRing}&password={password}&newKeyRing={newKeyRing}&email={email}",
			"method":"put",
			"timeout":15000
		},
		"deleteIndex":{
			"url":"/users/{username}/deleteIndex",
			"method":"get",
			"timeout":15000
		},
		"getUserProperty":{
			"url":"/users/{username}/properties/{property}",
			"method":"get",
			"timeout":15000
		},
		"setUserProperty":{
			"url":"/users/{username}/properties/{property}/{value}",
			"method":"post",
			"timeout":15000
		},
		"deleteUserProperty":{
			"url":"/users/properties/{property}",
			"method":"delete",
			"timeout":15000
		},
		"keySrvLogs":{
			"url":"/users/{username}/keysrvlogs",
			"method":"get",
			"timeout":15000
		},
		"datasources":{
			"url":"/datasources",
			"method":"get",
			"timeout":15000
		},
		"datasourceProfiles":{
			"url":"/datasources/{username}/profiles",
			"method":"get",
			"timeout":15000
		},
		"deleteDatasourceProfile":{
			"url":"/datasources/{username}/profiles/{datasourceProfileId}",
			"method":"delete",
			"timeout":15000
		},
		"datasourceProfileOptions":{
			"url":"/datasources/{username}/profiles/{profileId}/options",
			"method":"post",
			"timeout":15000
		},
		"authDatasource":{
			"url":"/datasources/{username}/{datasourceId}/auth?profileName={profileName}&keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"authDatasourcePost":{
			"url":"/datasources/{username}/{profileId}/auth/post?keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"getDatasinks":{
			"url":"/datasinks",
			"method":"get",
			"timeout":15000
		},
		"getUsersDatasinks":{
			"url":"/datasinks/{username}/profiles",
			"method":"get",
			"timeout":15000
		},
		"deleteDatasink":{
			"url":"/datasinks/{username}/profiles/{profileId}",
			"method":"delete",
			"timeout":15000
		},
		"authDatasink":{
			"url":"/datasinks/{username}/{datasinkId}/auth?profileName={profileName}&keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"authDatasinkPost":{
			"url":"/datasinks/{username}/{profileId}/auth/post?keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"actions":{
			"url":"/actions",
			"method":"get",
			"timeout":15000
		},
		"actionOptions":{
			"url":"/actions/{actionId}/options",
			"method":"get",
			"timeout":15000
		},
		"setActionOptions":{
			"url":"/actions/{actionId}/options/{jobId}",
			"method":"put",
			"timeout":15000
		},
		"getActionOptions":{
			"url":"/actions/{actionId}/storedOptions/{jobId}",
			"method":"get",
			"timeout":15000
		},
		"jobs":{
			"url":"/jobs/{username}",
			"method":"get",
			"timeout":15000
		},
		"jobDetails":{
			"url":"/jobs/{username}/{jobId}",
			"method":"get",
			"timeout":15000
		},
		"changeJob":{
			"url":"/jobs/{username}/{jobId}",
			"method":"put",
			"timeout":15000
		},
		"createJob":{
			"url":"/jobs/{username}?sourceProfiles={sourceProfiles}&actions={actions}&sinkProfileId={sinkProfileId}&timeExpression{timeExpression}&keyRing{keyRing}&jobTitle={jobTitle}{OPTIONS}",
			"method":"post",
			"timeout":15000
		},
		"deleteJob":{
			"url":"/jobs/{username}/{jobId}",
			"method":"delete",
			"timeout":15000
		},
		"jobStatus":{
			"url":"/jobs/{username}/{jobId}/status?fromDate={fromDate}&toDate={toDate}",
			"method":"get",
			"timeout":15000
		},
		"jobsStatus":{
			"url":"/jobs/{username}/status",
			"method":"get",
			"timeout":15000
		},
		"fileDetails":{
			"url":"/jobs/{username}/{fileId}/details",
			"method":"get",
			"timeout":15000
		},
		"jobStatusOverview":{
			"url":"/jobs/{username}/status/overview",
			"method":"get",
			"timeout":15000
		},
		"search":{
			"url":"/backups/{username}/search?keyRing={keyRing}&query={query}",
			"method":"post",
			"timeout":15000
		},
		"getSearch":{
			"url":"/backups/{username}/{searchId}/query?source={source}&type={type}&job={job}",
			"method":"get",
			"timeout":15000
		},
		"validateJob":{
			"url":"/jobs/{username}/validate/{jobId}?keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"validateDatasource":{
			"url":"/datasources/{username}/validate/{profileId}?keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"validateDatasink":{
			"url":"/datasinks/{username}/validate/{profileId}?keyRing={keyRing}",
			"method":"post",
			"timeout":15000
		},
		"sendMail":{
			"url":"/mails/send/{format}?to={to}&subject={subject}?message={message}",
			"method":"post",
			"timeout":15000
		},
		"getFileThumbnail":{
			"url":"/thumbnails/{username}/{fileID}",
			"method":"get",
			"timeout":15000
		}
	}
}