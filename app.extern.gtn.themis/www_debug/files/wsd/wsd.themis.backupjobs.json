{
	"template": 
	{
		"url": "string url",
		"method": "string method",
		"timeout": "int timeout"
	},

	"getAllBackupjobs": 
	{
		"url": "/backmeup-service-rest/backupjobs/",
		"method": "get",
		"timeout": "7000"
	},

	"getBackupjobsCreated": 
	{
		"url": "/backmeup-service-rest/backupjobs?jobStatus=CREATED&expand={expand}",
		"method": "get",
		"timeout": "7000"
	},

	"getBackupjobsActive": 
	{
		"url": "/backmeup-service-rest/backupjobs?jobStatus=ACTIVE&expand={expand}",
		"method": "get",
		"timeout": "7000"
	},

	"getBackupjobsPaused": 
	{
		"url": "/backmeup-service-rest/backupjobs?jobStatus=PAUSED&expand={expand}",
		"method": "get",
		"timeout": "7000"
	},

	"getBackupjobsNeedUserInteraction": 
	{
		"url": "/backmeup-service-rest/backupjobs?jobStatus=NEEDS_USER_INTERACTION&expand={expand}",
		"method": "get",
		"timeout": "7000"
	},

	"getBackupjob": 
	{
		"url": "/backmeup-service-rest/backupjobs/{jobId}?expandUser={expandUser}&expandToken={expandToken}&expandProfiles={expandProfiles}&expandProtocol={expandProtocol}",
		"method": "get",
		"timeout": "7000"
	},

	"createBackupjob": 
	{
		"url": "/backmeup-service-rest/backupjobs?jobTitle={jobTitle}&schedule={schedule}&start={start}&source={source}&actions={actions}&sink={sink}",
		"method": "post",
		"timeout": "7000"
	}
}